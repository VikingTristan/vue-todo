{
    "env": {
        "browser": true,
        "node": true
    },
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "extends": [
        "plugin:vue/strongly-recommended",
    ],
    "rules": {
        "vue/html-indent": ["error", "tab"],
        "vue/require-prop-types": false,
        "indent": [
            "error",
            "tab",
            {
                "SwitchCase": 1
            }
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "prefer-const": [
            "warn"
        ],
        "no-var": [
            "warn"
        ],
        "no-undef": [
            "warn"
        ]

    }
}